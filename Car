class Car{

    String carBrand = "";
    int enginePower = 0;
    double maxSpeed = 0;
    double price = 0;
    
    public void carBrand(String inBrand){
    
        carBrand = inBrand;
    }    
    
    public void enginePower(int inPower){
        
        enginePower = inPower;
    }
    
    public void maxSpeed(double inSpeed){
        
        maxSpeed = inSpeed;
    }
    
    public void price(double inPrice){
        
        price = inPrice;
    }
    
    
    
    
